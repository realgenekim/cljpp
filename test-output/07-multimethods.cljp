PUSH-( ns examples.polymorphism POP

PUSH-( defmulti render :type POP

PUSH-( defmethod render :button PUSH-[ component POP
  PUSH-{ :html PUSH-( str "<button>" PUSH-( :label component POP "</button>" POP POP
POP

PUSH-( defmethod render :input PUSH-[ component POP
  PUSH-{ :html PUSH-( str "<input type='" PUSH-( :input-type component "text" POP "'>" POP POP
POP

PUSH-( defmethod render :default PUSH-[ component POP
  PUSH-{ :html PUSH-( str "<div>" PUSH-( :content component "Empty" POP "</div>" POP POP
POP

PUSH-( defn render-form PUSH-[ components POP
  PUSH-( map render components POP
POP
