(ns examples.program29
  (:require [clojure.core.async :as async :refer [go go-loop chan <! >! alts! close!]]))

(defn channel-coordination []
  (let [ch1 (chan)
        ch2 (chan)
        ch3 (chan)
        output (chan)
        results (atom [])]
    
    (go-loop [n 0]
      (when (< n 3)
        (>! ch1 {:source :ch1 :value n})
        (recur (inc n)))
      (close! ch1))
    
    (go-loop [n 10]
      (when (< n 13)
        (>! ch2 {:source :ch2 :value n})
        (recur (inc n)))
      (close! ch2))
    
    (go-loop [n 20]
      (when (< n 23)
        (>! ch3 {:source :ch3 :value n})
        (recur (inc n)))
      (close! ch3))
    
    (go-loop []
      (let [[val port] (alts! [ch1 ch2 ch3])]
        (if val
          (do
            (swap! results conj val)
            (recur))
          (do
            (>! output @results)
            (close! output)))))
    
    output))
