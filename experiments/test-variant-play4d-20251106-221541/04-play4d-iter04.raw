PUSH-( ns examples.program4 POP-ALL

PUSH-( def users
  PUSH-[
    PUSH-{ :name "Alice" :age 25 :role "admin" POP
    PUSH-{ :name "Bob" :age 17 :role "user" POP
    PUSH-{ :name "Charlie" :age 30 :role "moderator" POP
    PUSH-{ :name "Diana" :age 16 :role "user" POP
    PUSH-{ :name "Eve" :age 22 :role "admin" POP
  POP
POP-ALL

PUSH-( defn active-users PUSH-[ users POP
  PUSH-( filter PUSH-( fn PUSH-[ user POP
    PUSH-( > PUSH-( :age user POP 18 POP
  POP users POP
POP-ALL

PUSH-( defn user-names PUSH-[ users POP
  PUSH-( map :name users POP
POP-ALL
