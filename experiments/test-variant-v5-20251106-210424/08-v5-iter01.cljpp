PUSH-( ns examples.program8 POP

PUSH-( defn process-user PUSH-[ PUSH-{ :keys PUSH-[ name age POP :as user POP POP
  PUSH-{ :greeting PUSH-( str "Hello, " name POP
         :info PUSH-( str "Age: " age POP
         :original user POP
POP

PUSH-( defn process-items PUSH-[ PUSH-[ first second & rest POP POP
  PUSH-{ :first first
         :second second
         :rest rest
         :count PUSH-( + 2 PUSH-( count rest POP POP POP
POP

PUSH-( defn nested-extract PUSH-[ PUSH-{ :keys PUSH-[ address POP POP POP
  PUSH-( let PUSH-[ PUSH-{ :keys PUSH-[ street city state POP POP address POP
    PUSH-{ :street street
           :city city
           :state state
           :full PUSH-( str street ", " city ", " state POP POP
  POP
POP
