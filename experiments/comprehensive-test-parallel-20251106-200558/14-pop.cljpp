PUSH-( ns examples.program14 POP

PUSH-( defprotocol Drawable
  PUSH-( draw PUSH-[ this POP "Draw the shape" POP
  PUSH-( bounds PUSH-[ this POP "Get bounding box as [x y width height]" POP
POP

PUSH-( defrecord Circle PUSH-[ x y radius POP
  Drawable
  PUSH-( draw PUSH-[ this POP
    PUSH-( println PUSH-( str "Circle at (" PUSH-( :x this POP ", " PUSH-( :y this POP ") with radius " PUSH-( :radius this POP POP POP
  POP
  PUSH-( bounds PUSH-[ this POP
    PUSH-[ PUSH-( - PUSH-( :x this POP PUSH-( :radius this POP POP
           PUSH-( - PUSH-( :y this POP PUSH-( :radius this POP POP
           PUSH-( * 2 PUSH-( :radius this POP POP
           PUSH-( * 2 PUSH-( :radius this POP POP POP
  POP
POP

PUSH-( defrecord Rectangle PUSH-[ x y width height POP
  Drawable
  PUSH-( draw PUSH-[ this POP
    PUSH-( println PUSH-( str "Rectangle at (" PUSH-( :x this POP ", " PUSH-( :y this POP ") with size " PUSH-( :width this POP "x" PUSH-( :height this POP POP POP
  POP
  PUSH-( bounds PUSH-[ this POP
    PUSH-[ PUSH-( :x this POP PUSH-( :y this POP PUSH-( :width this POP PUSH-( :height this POP POP
  POP
POP

PUSH-( defn -main PUSH-[ & args POP
  PUSH-( let PUSH-[ c PUSH-( ->Circle 10 20 5 POP
             r PUSH-( ->Rectangle 0 0 30 40 POP POP
    PUSH-( draw c POP
    PUSH-( println PUSH-( str "Circle bounds: " PUSH-( bounds c POP POP POP
    PUSH-( draw r POP
    PUSH-( println PUSH-( str "Rectangle bounds: " PUSH-( bounds r POP POP POP
  POP
POP
