PUSH-( ns examples.program8 POP

PUSH-( defn process-user PUSH-[ PUSH-{ :keys PUSH-[ name age POP :as user POP POP
  PUSH-{ :greeting PUSH-( str "Hello, " name POP
         :info PUSH-( str "Age: " age POP
         :original user POP
POP

PUSH-( defn process-items PUSH-[ PUSH-[ first second & rest POP POP
  PUSH-{ :first first
         :second second
         :rest rest
         :total PUSH-( + 2 PUSH-( count rest POP POP POP
POP

PUSH-( defn nested-extract PUSH-[ PUSH-{ PUSH-{ :keys PUSH-[ city state POP POP :address POP POP
  PUSH-( str city ", " state POP
POP