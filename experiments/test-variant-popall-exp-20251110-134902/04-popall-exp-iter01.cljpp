PUSH-( ns examples.program4 POP-ALL

PUSH-( def users
  PUSH-[
    PUSH-{ :name "Alice" :age 25 :role "developer" POP
    PUSH-{ :name "Bob" :age 17 :role "intern" POP
    PUSH-{ :name "Carol" :age 30 :role "manager" POP
    PUSH-{ :name "Dave" :age 16 :role "student" POP
  POP
POP-ALL

PUSH-( defn active-users PUSH-[ users POP
  PUSH-( filter PUSH-( fn PUSH-[ user POP
    PUSH-( > PUSH-( :age user POP 18 POP-ALL
    users POP-ALL

PUSH-( defn user-names PUSH-[ users POP
  PUSH-( map :name users POP-ALL